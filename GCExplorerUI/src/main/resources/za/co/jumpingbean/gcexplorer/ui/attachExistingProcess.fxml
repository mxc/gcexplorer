<?import java.lang.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox alignment="CENTER" prefHeight="300.0" prefWidth="419.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
<children><ListView fx:id="lstLocalProcesses" prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER">
<VBox.margin>
<Insets bottom="5.0" />
</VBox.margin></ListView><Button fx:id="btnConnectLocalProcess" mnemonicParsing="false" text="Connect Local Process" /><Separator prefWidth="200.0">
<VBox.margin>
<Insets top="5.0" />
</VBox.margin></Separator><HBox prefHeight="100.0" prefWidth="200.0">
<children><Label alignment="CENTER" prefHeight="15.0" prefWidth="106.0" text="Remote URL">
<HBox.margin>
<Insets right="25.0" />
</HBox.margin></Label><TextField fx:id="txtRemoteURL" prefHeight="25.0" prefWidth="232.0" promptText="Enter the targets remote URL" />
</children>
<opaqueInsets>
<Insets top="10.0" />
</opaqueInsets>
<VBox.margin>
<Insets bottom="10.0" top="10.0" />
</VBox.margin></HBox><HBox prefHeight="100.0" prefWidth="200.0">
<children><Label text="Username">
<HBox.margin>
<Insets right="10.0" />
</HBox.margin></Label><TextField fx:id="txtUsername" prefWidth="120.0" promptText="JMX user name">
<HBox.margin>
<Insets right="10.0" />
</HBox.margin></TextField><Label alignment="CENTER" text="Password">
<HBox.margin>
<Insets right="10.0" />
</HBox.margin></Label><PasswordField fx:id="txtPassword" prefWidth="120.0" promptText="JMX user password" />
</children></HBox><Button fx:id="btnConnectRemoteProcess" mnemonicParsing="false" text="Connect Remote Process" BorderPane.alignment="CENTER" />
</children>
<padding>
<Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
</padding>
</VBox>
